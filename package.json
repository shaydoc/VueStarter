{
  "name": "vue-router",
  "version": "2.3.0",
  "description": "Official router for Vue.js 2",
  "author": "Evan You",
  "license": "MIT",
  "main": "dist/vue-router.common.js",
  "module": "dist/vue-router.esm.js",
  "unpkg": "dist/vue-router.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuejs/vue-router.git"
  },
  "typings": "types/index.d.ts",
  "files": [
    "src",
    "dist/*.js",
    "types/*.d.ts"
  ],
  "keywords": [
    "vue",
    "router",
    "routing"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development ENV_LABEL=dev node server.js ",
    "dev:dist": "rollup -wm -c build/dev.config.js",
    "build": "cross-env NODE_ENV=production ENV_LABEL=hash PLATFORM=web webpack ",
    "lint": "eslint src examples",
    "test": "npm run lint && flow check && npm run test:unit && npm run test:e2e && npm run test:types",
    "test:unit": "jasmine JASMINE_CONFIG_PATH=test/unit/jasmine.json",
    "test:e2e": "node test/e2e/runner.js",
    "test:types": "tsc -p types/test",
    "docs": "cd docs && gitbook install && gitbook serve",
    "docs:deploy": "bash ./build/update-docs.sh",
    "release": "bash build/release.sh"
  },
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.11.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-flow-vue": "^1.0.0",
    "babel-preset-stage-0": "^6.1.2",
    "buble": "^0.15.2",
    "chart.js": "^2.5.0",
    "chromedriver": "^2.21.2",
    "cross-env": "^4.0.0",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "es6-promise": "^4.0.5",
    "eslint": "^3.0.1",
    "eslint-config-vue": "^2.0.1",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-vue": "^2.0.1",
    "express": "^4.14.0",
    "express-http-proxy": "^0.11.0",
    "express-urlrewrite": "^1.2.0",
    "flow-bin": "^0.40.0",
    "gitbook-plugin-edit-link": "^2.0.2",
    "gitbook-plugin-github": "^3.0.0",
    "jasmine": "2.5.3",
    "jquery": "^3.1.1",
    "moment": "^2.18.1",
    "nightwatch": "^0.9.5",
    "nightwatch-helpers": "^1.0.0",
    "path-to-regexp": "^1.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "ramda": "^0.23.0",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-watch": "^3.2.2",
    "selenium-server": "^2.53.1",
    "simple-vue-validator": "^0.13.1",
    "style-loader": "^0.18.1",
    "typescript": "^2.0.3",
    "uglify-js": "^2.7.0",
    "vee-validate": "^2.0.0-beta.25",
    "vue": "^2.3.3",
    "vue-button-spinner": "^1.2.0",
    "vue-chartjs": "^2.6.2",
    "vue-loader": "^11.0.0",
    "vue-router": "^2.3.0",
    "vue-spinner": "^1.0.2",
    "vue-template-compiler": "^2.3.3",
    "vue-toast": "^3.1.0",
    "vue-wysiwyg": "^1.1.2",
    "vuelidate": "^0.3.1",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.17.1"
  },
  "dependencies": {
    "vue-quill-editor": "^2.2.1",
    "vue-toasted": "^1.0.27"
  }
}
